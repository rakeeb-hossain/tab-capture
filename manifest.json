{
    "manifest_version": 2,
    "name": "tab-capture",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      }
      
}
  